    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="~/pagination/src/script.js"></script>
    <link href="~/pagination/src/style.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700%7cOpen+Sans:400,700" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="~/gaming-template/assets/vendor/bootstrap/dist/css/bootstrap.min.css">
    <script defer src="~/gaming-template/assets/vendor/fontawesome-free/js/all.js"></script>
    <script defer src="~/gaming-template/assets/vendor/fontawesome-free/js/v4-shims.js"></script>
    <link rel="stylesheet" href="~/gaming-template/assets/vendor/ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="~/gaming-template/assets/vendor/flickity/dist/flickity.min.css">
    <link rel="stylesheet" type="text/css" href="~/gaming-template/assets/vendor/photoswipe/dist/photoswipe.css">
    <link rel="stylesheet" type="text/css" href="~/gaming-template/assets/vendor/photoswipe/dist/default-skin/default-skin.css">
    <link rel="stylesheet" href="~/gaming-template/assets/vendor/bootstrap-slider/dist/css/bootstrap-slider.min.css">
    <link rel="stylesheet" type="text/css" href="~/gaming-template/assets/vendor/summernote/dist/summernote-bs4.css">
    <link rel="stylesheet" href="~/gaming-template/assets/css/goodgames.css">
    <link rel="stylesheet" href="~/gaming-template/assets/css/custom.css">
    <script src="~/gaming-template/assets/vendor/jquery/dist/jquery.min.js"></script>


</head>


<header class="nk-header nk-header-opaque">
    <nav class="nk-navbar nk-navbar-top nk-navbar-sticky nk-navbar-autohide nk-navbar-transparent">
        <div class="container">
            <div class="nk-nav-table">

                <a asp-controller="Home" asp-action="Index" class="nk-nav-logo">
                    <img src="~/gaming-template/assets/images/skull-removebg-preview.png" alt="GoodGames" width="75">
                </a>

                <ul class="nk-nav nk-nav-right d-none d-lg-table-cell" data-nav-mobile="#nk-nav-mobile">

                    <li>
                        <a asp-controller="Home" asp-action="Index">
                            Ana Sayfa

                        </a>
                    </li>
                    <li>
                        <a asp-controller="Blog" asp-action="Index">
                            Blog
                        </a>
                    </li>
                    <li>
                        <a asp-controller="Blog" asp-action="Index">
                            Haberler
                        </a>
                    </li>
                    <li>
                        <a asp-controller="Blog" asp-action="Index">
                            Hakkımızda
                        </a>
                    </li>
                    <li>
                        <a asp-controller="Blog" asp-action="Index">
                            İletişim
                        </a>
                    </li>
                </ul>
                <ul class="nk-nav nk-nav-right nk-nav-icons">

                    <li class="single-icon d-lg-none">
                        <a href="#" class="no-link-effect" data-nav-toggle="#nk-nav-mobile">
                            <span class="nk-icon-burger">
                                <span class="nk-t-1"></span>
                                <span class="nk-t-2"></span>
                                <span class="nk-t-3"></span>
                            </span>
                        </a>
                    </li>


                </ul>
            </div>
        </div>
    </nav>
    <div id="nk-nav-mobile" class="nk-navbar nk-navbar-side nk-navbar-right-side nk-navbar-overlay-content d-lg-none">
        <div class="nano">
            <div class="nano-content">
                <a href="index.html" class="nk-nav-logo">
                    <img src="~/gaming-template/assets/images/logo.png" alt="" width="120">
                </a>
                <div class="nk-navbar-mobile-content">
                    <ul class="nk-nav">
                        
                    </ul>
                </div>
            </div>
        </div>
    </div>

</header>

<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer class="nk-footer">

    <div class="container">
        <div class="nk-gap-3"></div>
        <div class="row vertical-gap">
            @await Component.InvokeAsync("FooterPopularPosts")

            @await Component.InvokeAsync("FooterLatestPosts")
        </div>
        <div class="nk-gap-3"></div>
    </div>

    
</footer>

<img class="nk-page-background-top" src="~/gaming-template/assets/images/bg-top-3.png" alt="">
<img class="nk-page-background-bottom" src="~/gaming-template/assets/images/bg-bottom.png" alt="">

<div class="nk-modal modal fade" id="modalSearch" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-sm" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span class="ion-android-close"></span>
                </button>

                <h4 class="mb-0">Search</h4>

                <div class="nk-gap-1"></div>
                <form action="#" class="nk-form nk-form-style-1">
                    <input type="text" value="" name="search" class="form-control" placeholder="Type something and press Enter" autofocus>
                </form>
            </div>
        </div>
    </div>
</div>

